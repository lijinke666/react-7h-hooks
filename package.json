{
  "name": "react-7h-hooks",
  "version": "0.8.4",
  "description": "每天只工作7小时!",
  "repository": "https://github.com/lijinke666/react-7h-hooks.git",
  "bugs": {
    "url": "https://github.com/lijinke666/react-7h-hooks/issues"
  },
  "homepage": "https://react-7h-hooks.netlify.com/",
  "author": "Jinke.Li <1359518268@qq.com>",
  "license": "MIT",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "keywords": [
    "react",
    "react-hooks",
    "7h"
  ],
  "sideEffects": [
    "dist/*"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/react-7h-hooks.min.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "yarn dev",
    "dev": "dumi dev",
    "build": "yarn clean && yarn build:cjs && yarn build:es && yarn build:umd",
    "lint": "eslint src --ext '.js,.jsx,.ts,.tsx'",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --project tsconfig.build.json --noEmit",
    "build:cjs": "tsc --project tsconfig.build.json -m commonjs --outDir lib --declaration",
    "build:es": "tsc --project tsconfig.build.json -m esNext --outDir es --declaration",
    "build:umd": "webpack --config webpack.umd.config.js",
    "clean": "rimraf lib es dist",
    "release": "standard-version && git push --follow-tags origin master && npm publish",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "ci:coverage": "npm run coverage && codecov",
    "clean:docs": "rimraf _docs",
    "build:docs": "yarn clean:docs && dumi build",
    "bundlesize": "bundlesize"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/react-7h-hooks.min.js",
      "maxSize": "100 kB"
    }
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint:types && yarn test"
    }
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "history": "^4.10.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/enzyme": "^3.10.5",
    "@types/history": "^4.7.6",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.154",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "antd": "^4.3.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "bundlesize": "^0.18.0",
    "codecov": "^3.7.0",
    "commitlint": "^8.3.5",
    "dumi": "^1.0.29",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.7",
    "lodash": "^4.17.15",
    "prettier": "^2.0.5",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.0.8",
    "standard-version": "^8.0.0",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
