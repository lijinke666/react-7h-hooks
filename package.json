{
  "name": "react-7h-hooks",
  "version": "0.8.1",
  "description": "每天只工作7小时!",
  "repository": "https://github.com/lijinke666/react-7h-hooks.git",
  "bugs": {
    "url": "https://github.com/lijinke666/react-7h-hooks/issues"
  },
  "homepage": "https://react-7h-hooks.netlify.com/",
  "author": "Jinke.Li <1359518268@qq.com>",
  "license": "MIT",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "keywords": [
    "react",
    "react-hooks",
    "7h"
  ],
  "sideEffects": [
    "dist/*"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/react-7h-hooks.min.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "yarn dev",
    "dev": "dumi dev",
    "build": "yarn clean && yarn build:cjs && yarn build:es && yarn build:umd",
    "lint": "eslint src --ext '.js,.jsx,.ts,.tsx'",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir es",
    "build:umd": "webpack --config webpack.umd.config.js",
    "clean": "rimraf lib es dist",
    "release": "standard-version",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "ci:coverage": "npm run coverage && codecov",
    "clean:docs": "rimraf _docs",
    "build:docs": "yarn clean:docs && dumi build",
    "bundlesize": "bundlesize"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "tsc --noEmit",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/react-7h-hooks.min.js",
      "maxSize": "100 kB"
    }
  ],
  "release": {
    "branches": [
      "master",
      {
        "name": "next",
        "prerelease": "rc"
      }
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "history": "^4.10.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.8.0",
    "@babel/preset-typescript": "^7.8.0",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/npm": "^5.3.5",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/enzyme": "^3.10.4",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "antd": "^4.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "codecov": "^3.6.1",
    "commitlint": "^8.3.5",
    "dumi": "^1.0.8",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.0.6",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "semantic-release": "^16.0.1",
    "standard-version": "^7.0.1",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
